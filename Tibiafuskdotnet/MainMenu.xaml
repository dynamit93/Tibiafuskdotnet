<Window x:Class="Tibiafuskdotnet.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tibiafuskdotnet"
        mc:Ignorable="d"
        Title="MainMenu" Height="450" Width="800">
    <Grid>
        <Button x:Name="Cavebot_btn" Content="Cavebot" HorizontalAlignment="Left" Margin="44,58,0,0" VerticalAlignment="Top" Height="31" Width="66"/>
        <Button x:Name="targeting_btn" Content="Targeting" HorizontalAlignment="Left" Margin="115,58,0,0" VerticalAlignment="Top" Height="31" Width="66"/>
        <Button x:Name="Healing_btn" Content="Healing" HorizontalAlignment="Left" Margin="186,58,0,0" VerticalAlignment="Top" Height="31" Width="66" Click="Healing_btn_Click"/>
        <Button x:Name="Extras_btn" Content="Extras" HorizontalAlignment="Left" Margin="257,58,0,0" VerticalAlignment="Top" Height="31" Width="66" Click="Extras_btn_Click_1"/>
        <Label Target="{Binding Maxhp, Mode=OneWay}"  x:Name="maxhpValue" Content="{Binding Maxhp, Mode=OneWay}" HorizontalAlignment="Left" Margin="82,22,0,0" VerticalAlignment="Top" Width="155"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="-284,369,0,0" VerticalAlignment="Top"/>

       
    </Grid>
</Window>
