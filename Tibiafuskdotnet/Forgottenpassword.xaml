<Window x:Class="Tibiafuskdotnet.Forgottenpassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tibiafuskdotnet"
        mc:Ignorable="d"
        Title="Forgottenpassword" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Username" HorizontalAlignment="Left" Margin="338,121,0,0" Text="Username" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Email" HorizontalAlignment="Left" Margin="338,153,0,0" Text="Email" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="RestorePassword" Content="Reset Password" HorizontalAlignment="Left" Margin="355,184,0,0" VerticalAlignment="Top" Click="RestorePassword_Click"/>
    </Grid>
</Window>
