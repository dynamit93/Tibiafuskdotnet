<Window x:Class="Tibiafuskdotnet.MVVM.Views.PvpMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tibiafuskdotnet.MVVM.Views"
        mc:Ignorable="d"
        Title="PvpMenu" Height="300" Width="900">
    <Grid Margin="0,3,0,-3" >

        <Label Content="Min hp %" Margin="29,58,785,198"/>
        <TextBox x:Name="PvpHealMaxMin" Margin="19,87,785,128" Text="{Binding SelectedTarget.MinHp, UpdateSourceTrigger=PropertyChanged}"/>
        <Label Content="Max hp %" Margin="120,59,696,202"/>
        <TextBox x:Name="PvpHealMaxHp" Margin="120,87,679,128"/>
        <Label Content="Player Name" Margin="239,59,580,198"/>
        <TextBox x:Name="PvpHealPlayerTextbox" Margin="238,91,580,142"/>
        <Label Content="Enable PVP HEAL" Margin="320,58,473,202"/>
        <CheckBox x:Name="PvpCHeckBoxEnable" Margin="330,97,549,170"/>
        <Label Content="Healparalyze" Margin="418,72,372,185"/>
        <CheckBox x:Name="HealParalyze" Margin="450,104,427,160" Checked="HealParalyze_Checked" />
    </Grid>
</Window>
